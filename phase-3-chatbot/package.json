{
  "name": "ai-todo-chatbot",
  "version": "1.0.0",
  "description": "AI-Powered Todo Chatbot - Phase III",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:concurrent": "concurrently \"npm run dev\" \"npm run backend\"",
    "backend": "cd src/backend && uvicorn main:app --reload --port 8000",
    "backend:prod": "cd src/backend && uvicorn main:app --host 0.0.0.0 --port 8000",
    "setup": "npm install && cd src/backend && pip install -r requirements.txt",
    "tailwind:build": "npx tailwindcss -i ./src/app/globals.css -o ./src/app/globals.css --minify",
    "tailwind:watch": "npx tailwindcss -i ./src/app/globals.css -o ./src/app/globals.css --watch"
  },
  "keywords": [
    "nextjs",
    "fastapi",
    "todo",
    "web-app"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.0",
    "jose": "^6.1.3",
    "next": "^14.1.0",
    "openai": "^4.24.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
